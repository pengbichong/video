<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.pbc.video.dao.UserInfoMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="org.pbc.video.model.UserInfo">
        <id column="ui_id" property="uiId" />
        <result column="user_id" property="userId" />
        <result column="head_path" property="headPath" />
        <result column="email" property="email" />
        <result column="telephone" property="telephone" />
        <result column="nick_name" property="nickName" />
        <result column="sex" property="sex" />
        <result column="lastLogin_date" property="lastloginDate" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ui_id, user_id, head_path, email, telephone, nick_name, sex, lastLogin_date AS lastloginDate
    </sql>

</mapper>
